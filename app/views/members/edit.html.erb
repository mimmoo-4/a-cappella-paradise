<h2>プロフィール編集</h2>
<%= form_with model: @member do |f| %>  <div class="field">
  <div class="field">
    <%= f.label "ニックネーム" %><br>
    <%= f.text_field :nickname, autofocus:true, id:"inputNickname", placeholder:"ニックネーム" %>
  </div>
  <div class="field">
    <label for="inputImage">プロフィール画像</label><br>
    <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  </div>
  <div class="field">
    <%= f.label "居住地" %><br>
    <%= f.text_field :address, autofocus:true, id:"inputAddress", placeholder:"居住地(市町村まで)" %>
  </div>
  <div class="field">
    <%= f.label "自己紹介" %><br>
    <%= f.text_area :self_introduction, autofocus:true, id:"inputSelf_Introduction", placeholder:"自己紹介(空欄可)" %>
  </div>
  <div class="field">
    <%= f.label "メインパート" %><br>
    <%= f.text_field :main_part, autofocus:true, id:"inputMain_Part", placeholder:"メインパート(空欄可)" %>
  </div>

  <%= f.submit "変更を保存" %>
<% end %>